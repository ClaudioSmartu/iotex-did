pragma solidity ^0.4.24;

contract DecentralizedIdentifier{
    mapping(string => uint8) idStatus; // 1,2,3 represent 3 status
    constructor() public{

    }
    function createDID(string idStr) public payable returns(bool success) {
        if(idStatus[idStr] == 0)
        {
            idStatus[idStr] = 1;
            success = true;
        }
        else
        {
            success = false;
        }
    }
    function isExist(string idStr) public returns(bool success){
        if(idStatus[idStr] == 0)
        {
            success = false;
        }
        else
        {
            success = true;
        }
    }
}
